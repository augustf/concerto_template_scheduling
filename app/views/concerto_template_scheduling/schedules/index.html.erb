<section class="viewblock">
  <header class="viewblock-header">
    <div class="viewblock-header_right">
      <div class="button-padding">
      </div>
    </div>
    <h1 class="default-padding">
      <%= t(:all_model, :model => ConcertoTemplateScheduling::Schedule.model_name.human.pluralize) %>
    </h1>
  </header>
  <div class="viewblock-cont">
    <table>
      <thead>
        <tr>
          <th>Start time</th>
          <th>End time</th>
          <th>Data</th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        <% @schedules.each do |schedule| %>
          <tr>
            <td><%= schedule.start_time %></td>
            <td><%= schedule.end_time %></td>
            <td><%= schedule.data %></td>
            <td><%= link_to 'Show', schedule %></td>
            <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
            <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</section>
